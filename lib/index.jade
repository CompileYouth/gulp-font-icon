doctype html
html
    head
        link(rel="stylesheet", href= fontName + ".css")

    body(style="padding: 0; margin: 0;")
        ul.icons(style="margin: 0 auto; font-size: 24px; width: 360px; padding: 50px; background-color: #eee")
            - icons.forEach(function(icon) {
            li.icon(style="list-style: none; height: 50px; text-align: left")
                i(class= fontAlias + " " + fontAlias + "-" + icon)
                span= "." + fontAlias + " " + "." + fontAlias + "-" + icon
            - })
